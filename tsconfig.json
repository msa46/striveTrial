{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["es5", "es2015.promise", "dom"],
    "strict": true,
    "typeRoots": ["./types"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}